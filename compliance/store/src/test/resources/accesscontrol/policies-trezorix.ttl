@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix acl: <http://www.openrdf.org/schema/sesame/acl/> .
@prefix ex: <http://example.org/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2008/05/skos#> .
@prefix rna: <http://www.rnaproject.org/data/> .      

skos:broader a owl:TransitiveProperty ;
	rdfs:subPropertyOf skos:broaderTransitive ;
    owl:inverseOf skos:narrower .

skos:narrower a owl:TransitiveProperty .

skos:broader a acl:InheritanceProperty .

rna:accessAttribute a acl:AccessAttribute .

<http://www.rnaproject.org/data/applicationUser/56> a acl:User ;
    acl:username "trezorix"^^xsd:string ;
    acl:hasRole <http://www.rnaproject.org/data/userNodePermissionRole/56/Edit> ;
    acl:hasRole <http://www.rnaproject.org/data/userNodePermissionRole/56/View> .


rna:permission1 a acl:Permission ;
    acl:hasOperation acl:edit ;
    acl:hasMatch [ a acl:ObjectMatch ;
                   rna:accessAttribute <http://www.rnaproject.org/data/AccessAttribute/1> 
		   ] .

rna:pa1 a acl:PermissionAssignment ;
    acl:hasPermission rna:permission1 ;
    acl:toRole <http://www.rnaproject.org/data/userNodePermissionRole/56/Edit> .

rna:permission127 a acl:Permission ;
    acl:hasOperation acl:edit ;
    acl:hasMatch [ a acl:ObjectMatch ;
                   rna:accessAttribute <http://www.rnaproject.org/data/AccessAttribute/127> 
		   ] .

rna:pa127 a acl:PermissionAssignment ;
    acl:hasPermission rna:permission127 ;
    acl:toRole <http://www.rnaproject.org/data/userNodePermissionRole/56/Edit> .


